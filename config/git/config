[core]
	autocrlf = "input"
	editor = "vim"

[fetch]
	prune = true

[filter "lfs"]
	clean = "git-lfs clean -- %f"
	process = "git-lfs filter-process"
	required = true
	smudge = "git-lfs smudge -- %f"

[init]
	defaultBranch = "main"

[pull]
	prune = true
	rebase = true

[rebase]
	autoStash = true

